<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    >
    <data>
        <variable
            name="obj"
            type="com.example.assignments.objectpassactvity.Student" />
    </data>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="20dp"
        tools:context=".objectpassactvity.ObjectOutput">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="30dp"
            android:layout_marginTop="20dp"
            android:text="@{obj.name}"
            android:id="@+id/objOutName" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/objOutName"
            android:textSize="30dp"
            android:layout_marginTop="20dp"
            android:text="@{obj.email}"
            android:id="@+id/objOutEmail"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/objOutEmail"
            android:textSize="30dp"
            android:layout_marginTop="20dp"
            android:text="@{String.valueOf(obj.num)}"
            android:id="@+id/objOutPhone" />

    </RelativeLayout>


</layout>